{
	"name": "svelte-app",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"watch:tailwind": "postcss public/css/tailwind.css -o public/css/app.css -w",
		"build": "rollup -c",
		"dev": "HOST=0.0.0.0 rollup -c -w",
