		"@types/quill": "^2.0.9",
		"moment": "^2.29.3",
		"quill": "^1.3.7",
		"quill-image-uploader": "^1.2.2",
		"rollup-plugin-dev": "^2.0.4",
		"secure-ls": "^1.2.6",
		"sirv-cli": "^2.0.0",
		"svelte-routing": "^1.6.0",
		"tailwind": "^2.2.0"
	}
